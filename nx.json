{
    "extends": "@nrwl/workspace/presets/npm.json",
    "npmScope": "tmt",
    "tasksRunnerOptions": {
      "default": {
        "runner": "@nrwl/nx-cloud",
        "options": {
          "cacheableOperations": ["build", "test", "lint", "package", "prepare"],
          "canTrackAnalytics": false,
          "accessToken": ""
        }
      }
    },
    "targetDependencies": {
      "build": [
        {
          "target": "build",
          "inputs": ["!{projectRoot}/enterprise/**/*"],
          "projects": "dependencies"
        }
      ],
      "prepare": [
        {
          "target": "prepare",
          "projects": "dependencies"
        }
      ],
      "package": [
        {
          "target": "package",
          "projects": "dependencies"
        }
      ]
    },
    "affected": {
      "defaultBase": "main"
    },
    "targetDefaults": {
      "build-ee": {
        "dependsOn": ["^build-ee"]
      },
      "build": {
        "dependsOn": ["^build"]
      }
    }
  }
